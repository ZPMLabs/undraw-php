{
  "name": "ZPMLabs/undraw-php",
  "description": "Tiny client for undraw.co search (Next.js data routes) returning DTOs and SVG code on demand.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/simple-cache": "^3.0",
    "php-http/discovery": "^1.19"
  },
  "autoload": {
    "psr-4": {
      "Undraw\\": "src/"
    }
  },
  "suggest": {
    "guzzlehttp/guzzle": "^7.0 || ^8.0",
    "symfony/cache": "^6.0 || ^7.0",
    "nyholm/psr7": "^1.8",
    "guzzlehttp/psr7": "^2.6"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
